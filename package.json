{
  "name": "unexpected-mitm",
  "version": "8.2.0",
  "description": "Unexpected plugin for the mitm library",
  "author": "Andreas Lind <andreas@one.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/unexpectedjs/unexpected-mitm.git"
  },
  "main": "lib/unexpectedMitm.js",
  "scripts": {
    "lint": "jshint .",
    "test": "mocha --compilers md:unexpected-markdown test/*.js `find documentation -name '*.md'` && npm run lint",
    "travis": "npm test && npm run coverage && (<coverage/lcov.info coveralls || true)",
    "coverage": "NODE_ENV=development istanbul cover _mocha -- --reporter dot && echo google-chrome coverage/lcov-report/index.html",
    "generate-site": "generate-site --require ./bootstrap-unexpected-markdown.js",
    "update-examples": "generate-site --require ./bootstrap-unexpected-markdown.js --update-examples",
    "deploy-site": "deploy-site.sh"
  },
  "peerDependencies": {
    "unexpected": "^10.0.0",
    "unexpected-messy": "^5.0.0"
  },
  "devDependencies": {
    "coveralls": "2.11.2",
    "express": "4.13.4",
    "istanbul": "0.3.5",
    "jshint": "2.6.0",
    "mocha": "2.2.5",
    "pem": "1.7.2",
    "request": "2.53.0",
    "semver": "5.0.1",
    "sinon": "1.17.2",
    "unexpected": "10.8.2",
    "unexpected-documentation-site-generator": "4.0.0",
    "unexpected-express": "8.1.0",
    "unexpected-http": "5.2.0",
    "unexpected-markdown": "1.3.1",
    "unexpected-sinon": "10.1.1"
  },
  "dependencies": {
    "async": "0.9.0",
    "callsite": "1.0.0",
    "detect-indent": "3.0.0",
    "memoizesync": "0.5.0",
    "messy": "^6.6.1",
    "mitm-mwoc": "1.3.0",
    "passerror": "1.1.0",
    "underscore": "1.7.0",
    "unexpected-messy": "^5.0.0"
  }
}
